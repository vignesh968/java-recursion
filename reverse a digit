//reverse a digit

import java.util.*;
public class Main
{
  public static void main(String[]args)
  {
    Scanner s=new Scanner(System.in);
    int a=s.nextInt();
    rev(a);
    
  }
  static int rev(int n)
  {
    if(n<10)
    {
      System.out.print(n);
      return 0;
    }
    else
    {
      System.out.print(n%10);
      return rev(n/10);
    }
  }
}
